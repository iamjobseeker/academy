package condition.loop;

public class ForEx_01 {
	
	public static void main(String[] args) {
		
		// 반복문, Loop
		// 반복제어문
		// for, while, do-while
		
		// for loop
		
		// for(초기식; 조건식; 증감식) {
		//		반복수행할 코드
		// 		조건식이 참(true)일 경우 반복수행
		// }
		
		int i;
		for ( i=0; i<5; ++i ) {
			System.out.println("Hello: " + i);
		}
		
		int j;
		for ( j=0; j<5; ++j ) {
			System.out.println("Hi: " + i);
		}
		
		
		
		
	}

}
