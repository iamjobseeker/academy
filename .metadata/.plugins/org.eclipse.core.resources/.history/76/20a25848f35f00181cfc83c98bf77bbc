package condition;

public class SwitchEx_01 {

	public static void main(String[] args) {
		
		// switch문
		// switch-case문
		
		// switch(변수) {  
		// case 값1:
		// case 값2:
		// case 값3:
		// ...
		// default:
		// }
		
//		()안에는 정수값이 온다, char 포함
		
			
		int num = 20;
		switch(num) {
		case 10:
			System.out.println("값은: 10");
			break;
			
		case 20:
			System.out.println("값은: 20");
			break;
			
		case 30:
			System.out.println("값은: 30");
			break;
			
		default:
			System.out.println("디폴트, 적당한 값 없음");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
	
}
