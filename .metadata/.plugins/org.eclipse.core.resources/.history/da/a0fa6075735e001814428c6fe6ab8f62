package condition;

public class IfEx_02 {

	public static void main(String[] args) {
		
		int num=10;
		
		if(num!=10) {
			System.out.println("num is not 10");
		}
		
		System.out.println("just code");
		
		System.out.println("-------");
		
		int num1 = 11;
		int num2 = 22;
		
		if (num1 < num2) {
			System.out.println("num1 is smaller.");
		}
		
		
	}
	
}
