package condition;

public class SwitchEx_03 {
	
	public static void main(String[] args) {
		
		char ch = 'B';
		switch(ch) {
		case 'Q':
			System.out.println("대문자 큐");
			break;
			
		case 'q':	// == 113 (아스키 코드)
			System.out.println("소문자 큐");
			break;
		
		case 'a':
			System.out.println("에이");
			break;
			
		case 'B':
			System.out.println("비");
			break;
			
		}
		
	}
}
