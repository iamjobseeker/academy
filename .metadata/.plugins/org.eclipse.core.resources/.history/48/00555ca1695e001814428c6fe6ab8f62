package operator;

import java.util.Scanner;

public class mytest {

	public static void main(String[] args) {
		
		Scanner test = new Scanner(System.in);
		int id1;
		int id2;
		
		System.out.println("아이디를 입력하세요.");
		id1=test.nextInt();
		System.out.println("아이디 "+ id1);
		
		System.out.println("아이디를 입력하세요.\n (단, 아이디는 중복 불가)");
		id2=test.nextInt();
		if (id1==id2) {
			
			System.out.println("에러: 두 아이디가 같습니다");
		}
		
		else {
			System.out.println("변수" +id2+"가 입력되었습니다.");
		}
		
		System.out.println("사용자 아이디" +id1+id2);
		
	}
	
}
