package scanner;

import java.util.Scanner;

public class ScannerEx_02 {
	
	public static void main(String[] args) {
		
		// 입력 객체 Scanner
		Scanner input = new Scanner(System.in);
		
		System.out.print("Input: ");
		
		String str = input.nextLine();
		
		System.out.println("입력 데이터: " + str);
		
		
		
	}


}
