package variables;

public class VariablesEx_05 {
	
	public static void main(String[] args) {
		
		// string
		// 참조형 타입
		// 문자열을 다룰 수 있게 해주는 클래스
		String name;
		name="Alice";
		System.out.println(name);
		System.out.println("이름: "+name);
		
		int age =22;
		System.out.println("나이: "+age);
		
		String fruit;
		int price;
		int num;
		fruit="딸기";
		price=2000;
		num=10;
		System.out.println();
	}

}
